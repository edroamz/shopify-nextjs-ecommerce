{
  "extends": ["next/core-web-vitals", "next", "prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "caughtErrors": "none",
        "ignoreRestSiblings": true,
        "vars": "all"
      }
    ],
    "prefer-const": "error",
    "react-hooks/exhaustive-deps": "error"
  }
}
